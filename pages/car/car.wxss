.car-tabbar{
  width:100%;
  height:50px;
  background:#fff;
  position: fixed;
  bottom:0;
  left:0;
  z-index:99;
  display:-webkit-flex;
  display:flex;
  justify-content: space-between;
  align-items: center;
  border-top:1px solid #DDD;
}
.car-total{
  padding-left:15px;
}
.settlement{
  width:100px;
  text-align: center;
  height:50px;
  line-height:50px;
  color:#fff;
  background:#ff9800;
}
.car-img{
  width:160rpx;
  height:160rpx;
  background:#fafafa;
  border-radius:8px;
  margin-right:20rpx;
  overflow: hidden;
}
.car-name{
  display:-webkit-flex;
  display:flex;
  justify-content: space-between;
  line-height:48rpx;
}
.c-name{
  font-weight:bold;
}
.c-price{
  font-weight:bold;
  color:#ff9800;
}
.car-attr{
  font-size:30rpx;
  color:#ccc;
  margin-top:10rpx;
  min-height:70rpx;
}
.car-control{
  display:-webkit-flex;
  display:flex;
  justify-content: flex-end;
}
.disabledTarget{
  background:#ddd;
  pointer-events: none;
}
.nocart{

}
.nocart-icon{
  width:240rpx;
  height:240rpx;
  margin:50px auto 0;
}
.nocart-icon>image{
  width:240rpx;
  height:240rpx;
}
.nocart-tip{
  text-align: center;
  margin-top:20rpx;
  color:#ccc;
}