page{
  height:100%;
}
.address{
  color:#666;
  margin-bottom:16rpx;
}
.address-user{
  color:#666;
}
.addaddress{
  width:100%;
  height:50px;
  background:#fff;
  position:fixed;
  bottom:0;
  left:0;
  display:-webkit-flex;
  display:flex;
  align-items: center;
  justify-content: center;
}
.add-btn{
  display:inline-block;
  padding-left:34px;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:24px;
  background-position:left center;
}
.get-self{
  display:-webkit-flex;
  display:flex;
  justify-content: space-between;
  align-items: center;
  padding:20rpx 30rpx;
  box-sizing: border-box;
  background:#fff;
  margin-bottom:20rpx;
}

.noaddress{
  padding-top:100px;
  text-align: center;
  color:#999;
}
.noaddress>image{
  width:240rpx;
  height:240rpx;
  display:block;
  margin:0 auto;
}