.header{
  width:100%;
  text-align: center;
  position:fixed;
  top:0;
  left:0;
  z-index:99999;
}
.status{
  height:20px;
}
.header-nav{
  height:44px;
  text-align: center;
  display:-webkit-flex;
  display:flex;
  align-content: center;
  position:relative;
  align-items: center;
}
.title-nav{
  width:100%;
  color:#fff;
  font-size:32rpx;
}
.userAvatarUrl{
  width:32px;
  height:32px;
  border-radius:50%;
  overflow: hidden;
  position:absolute;
  left:15px;
}
.back-nav{
  display:inline-block;
  width:44px;
  height:100%;
  position:absolute;
  left:0;
}
.back-nav>image{
  width:24px;
  height:24px;
  position:absolute;
  top:50%;
  left:50%;
  transform: -webkit-translate(-50%,-50%);
  transform: translate(-50%,-50%);
}
.hover{
  background:rgba(0,0,0,0.1);
  opacity: 0.7;
}