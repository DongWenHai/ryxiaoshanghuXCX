.loginModel{
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  left:0;
  z-index:9999
}
.loginModelMask{
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  left:0;
  background:rgba(0,0,0,.3);
}
.loginModelContainer{
  width:56%;
  min-height:240rpx;
  background:#fff;
  border-radius:8rpx;
  box-shadow: 0 0 8rpx 0 #fff;
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%,-50%);
}
.modelTitle{
  font-size:28rpx;
  text-align: center;
  line-height:64rpx;
  color:#333;
}
.modelContent{
  padding:20rpx;
  font-size:30rpx;
  color:#666;
}
.modelBtnGroup{
  padding:0 30rpx 20rpx;
  height:64rpx;
}
.modelConfirm{
  float:right;
  color:#1AAD19;
  font-size:30rpx;
  line-height:64rpx;
  padding:0 20rpx;
  border-radius:8rpx;
}
.modelConfirm:active{
  background:rgba(0,0,0,.1);
}
button.modelConfirm{
  background:#fff;
}
button.modelConfirm::after{
  border:none;
}